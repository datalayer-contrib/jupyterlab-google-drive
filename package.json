{
  "name": "@jupyterlab/google-drive",
  "version": "1.0.0-pre.2",
  "description": "Cloud storage with JupyterLab through Google Drive",
  "keywords": [
    "google drive",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupyterlab/jupyterlab-google-drive",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab-google-drive/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Ian Rose",
  "files": [
    "lib/*/*d.ts",
    "lib/*/*.js",
    "lib/*.d.ts",
    "lib/*.js",
    "schema/*.json",
    "style/*.*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab-google-drive.git"
  },
  "scripts": {
    "build": "tsc",
    "build:test": "cd test && ./build-tests.sh",
    "clean": "rimraf lib",
    "precommit": "lint-staged",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "test": "cd test && ./run-tests.sh",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0-alpha.8",
    "@jupyterlab/apputils": "^1.0.0-alpha.8",
    "@jupyterlab/coreutils": "^3.0.0-alpha.8",
    "@jupyterlab/docmanager": "^1.0.0-alpha.8",
    "@jupyterlab/docregistry": "^1.0.0-alpha.8",
    "@jupyterlab/filebrowser": "^1.0.0-alpha.8",
    "@jupyterlab/mainmenu": "^1.0.0-alpha.8",
    "@jupyterlab/observables": "^2.2.0-alpha.8",
    "@jupyterlab/services": "^4.0.0-alpha.8",
    "@phosphor/algorithm": "^1.1.2",
    "@phosphor/commands": "^1.6.1",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/signaling": "^1.2.2",
    "@phosphor/widgets": "^1.6.0"
  },
  "devDependencies": {
    "@types/expect.js": "~0.3.29",
    "@types/gapi": "^0.0.33",
    "@types/gapi.auth2": "^0.0.40",
    "@types/mocha": "~2.2.44",
    "@types/node": "~8.0.47",
    "css-loader": "~0.28.7",
    "expect.js": "~0.3.1",
    "file-loader": "~0.10.1",
    "husky": "^0.14.3",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~0.2.3",
    "karma-firefox-launcher": "~0.1.7",
    "karma-mocha": "~0.2.2",
    "karma-mocha-reporter": "~1.3.0",
    "karma-sourcemap-loader": "~0.3.7",
    "lint-staged": "^7.2.0",
    "mocha": "~3.5.3",
    "prettier": "^1.13.7",
    "rimraf": "~2.6.2",
    "style-loader": "~0.13.2",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "~3.1.0",
    "url-loader": "~0.5.9",
    "webpack": "~2.7.0"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
